        var options = {
          // height: 400,
          sankey: {
            node: {
              colors: colors,
              label: { 
                fontName: 'Helvetica Neue', 
                fontSize: 15 
              }
            },
            link: {
              colorMode: 'gradient',
              colors: colors
            }
          },
          tooltip: {
            textStyle: {
              color: 'black',
              fontName: 'Helvetica Neue',
              fontSize: 15,
            }, 
            showColorCode: true
          }, 
        };

        // Instantiates and draws our chart, passing in some options.
        var chart = new google.visualization.Sankey(document.getElementById('sankey_basic'));
        chart.draw(data, options);
      }

